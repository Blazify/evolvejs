{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2018",
      "outDir": "dist",
      "lib": [
        "ESNext",
        "ESNext.Array",
        "ESNext.AsyncIterable",
        "ESNext.Intl",
        "ESNext.Symbol",
        "dom"
      ],
      "sourceMap": false,
      "inlineSourceMap": true,
      "inlineSources": true,
      "incremental": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "removeComments": true,
      "strict": false,
      "strictNullChecks": false,
      "noImplicitThis": true,
      "allowJs": true,
      "declaration": false,
      "skipLibCheck": true,
      "skipDefaultLibCheck": true
    },
    "include": [
      "Src/**/*"
    ],
    "exclude": [
      "node_modules",
    ]
  }